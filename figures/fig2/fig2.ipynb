      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Plot the Light Curves\n",
    "fig,ax = plt.subplots(4,figsize=(12,12),sharex='col',gridspec_kw={'hspace': 0.1, 'wspace': 0})\n",
    "\n",
    "# MeerKAT\n",
    "sub5sigma_index = np.where(MeerKAT_flux[0]/MeerKAT_flux[1] < 5.0)\n",
    "\n",
    "# 2021 Outburst \n",
    "ax[0].errorbar(Time(MeerKAT_times, format='mjd').datetime,MeerKAT_flux[0], yerr=MeerKAT_flux[1],fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='C0')\n",
    "ax[0].errorbar(Time(MeerKAT_times[sub5sigma_index], format='mjd').datetime,MeerKAT_flux[0][sub5sigma_index], yerr=MeerKAT_flux[1][sub5sigma_index],fmt='d', mec='k', mew=1.0, ecolor='k', lw=0.5, c='C0', ms=10)\n",
    "\n",
    "# 2023 Followup\n",
    "ax[0].axhline(MeerKAT_flux[0][-2], c='purple', ls=':')\n",
    "ax[0].axhspan(MeerKAT_flux[0][-2] - MeerKAT_flux[1][-2], MeerKAT_flux[0][-2] + MeerKAT_flux[1][-2], color='purple', alpha=0.25)\n",
    "ax[0].axhline(MeerKAT_flux[0][-1], c='teal', ls='--')\n",
    "ax[0].axhspan(MeerKAT_flux[0][-1] - MeerKAT_flux[1][-1], MeerKAT_flux[0][-1] + MeerKAT_flux[1][-1], color='teal', alpha=0.25)\n",
    "ax[0].set_ylabel('MeerKAT Flux Density\\n($\mu$Jy)', fontsize=12,fontfamily='serif')\n",
    "ax[0].set_ylim(0,260)\n",
    "\n",
    "# Swift-XRT\n",
    "ax[1].errorbar(Time(xrt_times, format='mjd').datetime,xrt_spectra['tot_flux'], yerr=[xrt_spectra['tot_flux_neg'],xrt_spectra['tot_flux_pos']],fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='k', label='Total Flux')\n",
    "ax[1].errorbar(Time(xrt_lowcount_times, format='mjd').datetime,xrt_lowcount['flux'], yerr=[xrt_lowcount['flux_neg'], xrt_lowcount['flux_pos']], fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='w', label='Total Flux')\n",
    "ax[1].set_ylabel('Swift-XRT Fluxes\\n[0.5-10keV]\\n(erg s$^{-1}$ cm$^{-2}$)', fontsize=12,fontfamily='serif')\n",
    "ax[1].set_ylim(-1.0e-10,2.0e-9)\n",
    "\n",
    "# Swift-XRT low flux inset\n",
    "left, bottom, width, height = [0.63, 0.575, 0.25, 0.1]\n",
    "ax_inset = fig.add_axes([left, bottom, width, height])\n",
    "ax_inset.set_xlim(59488, 59513)\n",
    "ax_inset.xaxis.set_minor_locator(AutoMinorLocator(3))\n",
    "ax_inset.tick_params(axis='both', which='minor', direction='in',length=5,width=1.5,top=True,right=True)\n",
    "ax_inset.tick_params(axis='both', which='major', direction='in',length=10,width=1.5,top=True,right=True, labelsize=12)\n",
    "ax_inset.errorbar(xrt_times[-2:],xrt_spectra['tot_flux'][-2:], yerr=[xrt_spectra['tot_flux_neg'][-2:],xrt_spectra['tot_flux_pos'][-2:]],fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='k', label='Total Flux')\n",
    "ax_inset.errorbar(xrt_lowcount_times,xrt_lowcount['flux'], yerr=[xrt_lowcount['flux_neg'], xrt_lowcount['flux_pos']], fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='w', label='Total Flux')\n",
    "ax_inset.set_yscale('log')\n",
    "ax_inset.minorticks_on()\n",
    "ax_inset.set_ylim(1e-13,9e-10)\n",
    "\n",
    "# MAXI/GSC\n",
    "ax[2].errorbar(Time(maxi[:,0], format='mjd').datetime, maxi[:,1], yerr=maxi[:,2], fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='green')\n",
    "ax[2].set_ylabel('MAXI-GSC Count Rate\\n[4-10keV]\\n(count s$^{-1}$ cm$^{-2}$)', fontsize=12,fontfamily='serif')\n",
    "ax[2].axhline(3 * np.median(maxi[:,2]) * 0.0, ls=':', c='k')\n",
    "ax[2].set_ylim(-0.025,0.09)\n",
    "\n",
    "# Swift-BAT\n",
    "ax[3].errorbar(Time(bat[:,0], format='mjd').datetime, bat[:,1], yerr=bat[:,2],fmt='o', mec='k', mew=1.0, ecolor='k', lw=0.5, c='red')\n",
    "ax[3].set_ylabel('Swift-BAT Count Rate\\n[15-50keV]\\n(count s$^{-1}$ cm$^{-2}$)', fontsize=12,fontfamily='serif')\n",
    "ax[3].axhline(3 * np.median(bat[:,2]) * 0.0, ls=':', c='k')\n",
    "ax[3].set_ylim(-0.007,0.02)\n",
    "\n",
    "# Make it Pretty\n",
    "for ax_i in ax:\n",
    "    #ax_i.xaxis.set_minor_locator(AutoMinorLocator(3))\n",
    "    ax_i.tick_params(axis='both', which='minor', direction='in',length=5,width=1.5,top=True,right=True)\n",
    "    ax_i.tick_params(axis='both', which='major', direction='in',length=10,width=1.5,top=True,right=True, labelsize=12, labelbottom=False)\n",
    "    ax_i.minorticks_on()\n",
    "    \n",
    "def plot2mjd(t):\n",
    "    '''Convert from matplotlib plot date to mjd'''\n",
    "    return Time(t, format=\"plot_date\", scale='utc').mjd\n",
    "\n",
    "\n",
    "def mjd2plot(mjd):\n",
    "    '''Convert from mjd to matplotlib plot'''\n",
    "    return Time(mjd, format=\"mjd\", scale='utc').plot_date\n",
    "\n",
    "# Format primary x-axis\n",
    "ax[0].set_xlabel('Observing Date (UTC)', fontsize=15, fontfamily='serif')    \n",
    "ax[0].set_xlim(Time(59345, format='mjd').datetime,Time(59525, format='mjd').datetime)\n",
    "ax[0].xaxis.set_major_locator(mdates.DayLocator(interval=32))\n",
    "ax[0].xaxis.set_label_position('top') \n",
    "xformatter = mdates.DateFormatter('%Y-%m-%d')\n",
    "plt.gcf().axes[0].xaxis.set_major_formatter(xformatter)\n",
    "ax[0].tick_params(axis='x', which='major',rotation=10, labelsize=12 ,labeltop=True, labelbottom=False)\n",
    "\n",
    "# Format secondary x-axis\n",
    "mjd_ax = ax[-1].secondary_xaxis('bottom', functions=(plot2mjd, mjd2plot))\n",
    "mjd_ax.set_xlabel('Observing Date (MJD)', fontsize=15, fontfamily='serif')  \n",
    "mjd_ax.tick_params(which='major', direction='in', length = 0.0, width = 0.0, labelsize=12)\n",
    "plt.draw()\n",
    "\n",
    "# Extract the labels\n",
    "mjd_ticks = []\n",
    "labels = ax[0].get_xticklabels(which='major')\n",
    "for lab in labels:\n",
    "    mjd_ticks.append(lab.get_text() + 'T00:00:00')\n",
    "\n",
    "# Line up MJD and Datetime labels \n",
    "mjd_ticks = (Time(mjd_ticks, format='isot').mjd).astype(int)\n",
    "mjd_ax.set_xticks(mjd_ticks, labels = mjd_ticks)\n",
    "plt.show()"
   ]
  },
