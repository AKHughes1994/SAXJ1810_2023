   "id": "ef094ff8",
   "metadata": {},
   "source": [
    "## Load in the data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "7a98242c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Swift-BAT light curve [15-50keV] -- columns: Time, Rate, Error\n",
    "bat  = np.loadtxt('../../analysis/bat-maxi/results/filtered_bat.txt')\n",
    "\n",
    "# MAXI/GSC light curve [4-10keV]\n",
    "maxi = np.loadtxt('../../analysis/bat-maxi/results/filtered_maxi.txt')\n",
    "\n",
    "# Swift-XRT spectral dictionary -- keys describe each entry \n",
    "with open('../../analysis/swift-xrt/results/fit_dict_full_Emin0.5.json') as jfile:\n",
    "    xrt_spectra = json.load(jfile)\n",
    "    \n",
    "for key in xrt_spectra.keys():\n",
    "    xrt_spectra[key] = np.array(xrt_spectra[key])\n",
    "xrt_spectra['tot_flux'] = xrt_spectra['flux'] + xrt_spectra['bb_flux']\n",
    "xrt_spectra['tot_flux_neg'] = np.sqrt(xrt_spectra['flux_neg']**2 + xrt_spectra['bb_flux_neg']**2)\n",
    "xrt_spectra['tot_flux_pos'] = np.sqrt(xrt_spectra['flux_pos']**2 + xrt_spectra['bb_flux_pos']**2)\n",
    "xrt_times = Time(xrt_spectra['times'], format='isot').mjd\n",
    "\n",
    "# Load in the lowcount xrt spectra\n",
    "with open('../../analysis/swift-xrt/results/fit_dict_lowcount_Emin0.5.json') as jfile:\n",
    "    xrt_lowcount = json.load(jfile)\n",
    "xrt_lowcount_times = Time(xrt_lowcount['times'], format='isot').mjd\n",
    "\n",
    "\n",
    "# MeerKAT Flux Densities \n",
    "MeerKAT_isot, _ = np.load('../../analysis/meerkat/results/imfit_times.npy')\n",
    "MeerKAT_times = Time(MeerKAT_isot, format='isot').mjd\n",
    "MeerKAT_flux = np.load('../../analysis/meerkat/results/imfit_flux_results.npy')"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "43b040be",
   "metadata": {},
   "source": [
    "## Plot the Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "85c946f8",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 864x864 with 5 Axes>"
      ]
